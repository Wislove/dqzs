}
enum UnionPositionEnum {
    UnionPositionEnum_Leader = 0;                   
    UnionPositionEnum_Deputy = 1;                   
    UnionPositionEnum_Elite = 2;                    
    UnionPositionEnum_Member = 3;                   
}
