message PlayerEmoticonDataMsg{
    repeated SingleEmoticonInfo emoticonList = 1;
}
message PlayerEmoticonDataSync{
    optional PlayerEmoticonDataMsg syncData = 1;
}
message SingleEmoticonInfo{
    optional int32 id = 1;
    optional int64 endTime = 2;
}
